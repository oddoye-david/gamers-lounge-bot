{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "skipLibCheck": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es5",
      "es6"
    ],
    "rootDir": "./",
    "types": [
      "node"
    ],
  },
  "exclude": [
    "node_modules",
    "./test",
    "./public"
  ]
}
